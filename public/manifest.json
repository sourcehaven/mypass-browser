{
  "short_name": "MyPass",
  "name": "MyPass Browser",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open MyPass"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "activeTab", "scripting", "webRequest", "storage"],
  "host_permissions": ["https://*/*", "http://*/*"]
}
